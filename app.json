{
  "expo": {
    "name": "BoulderView",
    "scheme": "boulderview",
    "slug": "BoulderView",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "owner": "boulderview2023",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.anonymous.BoulderView"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.BoulderView"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "e822daa8-0d06-4d87-b9dd-89c35b42d33d"
      }
    },
    "runtimeVersion": {
      "policy": "sdkVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/e822daa8-0d06-4d87-b9dd-89c35b42d33d"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "BoulderView accesses your photos and videos to let you share them with your friends."
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow BoulderView to access your camera.",
          "microphonePermission": "Allow BoulderView to access your microphone"
        }
      ]
    ]
  }
}
